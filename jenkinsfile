pipeline {
  agent any
 
 
 tools {
      maven 'M2_HOME'
          }

  stages {
    stage('CheckOut') {
      steps {
        echo 'Checkout the source code from GitHub'
	git 'https://github.com/noor1718/star-agile-health-care.git'
            }
    }
   
   stage('Package and Build App') {
      steps {
        echo 'Build and Package App'
        sh 'mvn clean package'
           }
   	 }
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 }
	 }
	
		 


